    {
      "parameters": {
        "jsCode": "// Get data from previous nodes\nconst embedding = $input.first().json.embedding;\nconst originalQuery = $input.first().json.query;\n\n// Create hybrid search query (combining vector search with text search)\nconst esQuery = {\n  query: {\n    bool: {\n      should: [\n        // Vector search component (70% weight)\n        {\n          script_score: {\n            query: { match_all: {} },\n            script: {\n              source: \"cosineSimilarity(params.query_vector, 'vector') + 1.0\",\n              params: { query_vector: embedding }\n            },\n            boost: 0.7\n          }\n        },\n        // Text search component (30% weight)\n        {\n          multi_match: {\n            query: originalQuery,\n            fields: [\"text^3\", \"metadata.doc_title^2\", \"metadata.book_name\", \"metadata.page_name\"],\n            type: \"best_fields\",\n            boost: 0.3\n          }\n        }\n      ]\n    }\n  },\n  size: 5\n};\n\nreturn {\n  json: {\n    esQuery: esQuery,\n    originalQuery: originalQuery\n  }\n};"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
